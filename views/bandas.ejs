<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% bandas.forEach(element => { %>
       <li>
        <a href="bandas/<%= element.id %>"><p><%= element.nombre %></p>
        <img src="/<%= element.cover %>" alt="" style="width: 200px">
        </a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>